{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch FutureTradesTracker",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/bin/Debug/net9.0/FutureTradesTracker.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "DOTNET_ENVIRONMENT": "Development"
            }
        },
        {
            "name": "Launch FutureTradesTracker (Release)",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/src/bin/Release/net9.0/FutureTradesTracker.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "DOTNET_ENVIRONMENT": "Production"
            }
        }
    ]
}
