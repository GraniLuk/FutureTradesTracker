{
  "BingXApi": {
    "ApiKey": "",
    "SecretKey": "",
    "BaseUrl": "https://open-api.bingx.com"
  },
  "BybitApi": {
    "ApiKey": "",
    "SecretKey": "",
    "BaseUrl": "https://api.bybit.com"
  },
  "RateLimiting": {
    "BingXRequestsPerSecond": 2,
    "BybitRequestsPerSecond": 5,
    "RetryAttempts": 3,
    "RetryDelaySeconds": 2
  },
  "IntegrationTests": {
    "TestSymbols": ["BTC-USDT", "ETH-USDT", "BNB-USDT"],
    "MaxTestExecutionTimeSeconds": 30,
    "EnablePerformanceTests": true,
    "EnableErrorHandlingTests": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "CryptoPositionAnalysis.Services": "Debug"
    }
  }
}
